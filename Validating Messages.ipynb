   "id": "25051ec8",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.feature_extraction.text import CountVectorizer \n",
    "vectorizer=CountVectorizer(analyzer=analysing_message)\n",
    "x_train=vectorizer.fit_transform(x_train)\n",
    "x_test = vectorizer.transform(x_test)"
